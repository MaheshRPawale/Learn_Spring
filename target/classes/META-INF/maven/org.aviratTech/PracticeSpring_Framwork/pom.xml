<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.aviratTech</groupId>
  <artifactId>PracticeSpring_Framwork</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>PracticeSpring_Framwork</name>
  <description>PracticeSpring_Framwork in manually config and Annotation config</description>
  
  
  
  
  <build>
    <plugins>
        <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-jar-plugin</artifactId>
            <version>3.2.0</version>
            <configuration>
                <archive>
                    <manifest>
                        <mainClass>demo.Test</mainClass>
                    </manifest>
                </archive>
            </configuration>
        </plugin>
    </plugins>
</build>
  
 <dependencies>
    <!-- Spring Context (core container) -->
    <dependency>
        <groupId>org.springframework</groupId>
        <artifactId>spring-context</artifactId>
        <version>5.3.34</version> <!-- or another stable version compatible with your Java -->
    </dependency>

    <!-- Optional: Spring Beans, needed for @Component, etc. -->
    <dependency>
        <groupId>org.springframework</groupId>
        <artifactId>spring-beans</artifactId>
        <version>5.3.34</version>
    </dependency>
</dependencies>

  
  
</project>